            },
            "example" : "14:45:30.987654321"
          },
          "localTimeNullable" : {
            "type" : [ "string", "null" ],
            "format" : "local-time",
            "example" : "14:45:30.999999999",
            "externalDocs" : {
              "description" : "As defined by 'partial-time' in RFC3339",
              "url" : "https://www.rfc-editor.org/rfc/rfc3339.html#section-5.6"
            }
          }
        }
      }
    }
  }
